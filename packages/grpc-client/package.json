{
  "name": "@0xobelisk/grpc-client",
  "version": "1.2.0-pre.74",
  "description": "Toolkit for interacting with dubhe gRPC indexer",
  "keywords": [
    "sui",
    "obelisk labs",
    "move",
    "blockchain",
    "grpc",
    "indexer"
  ],
  "homepage": "https://github.com/0xobelisk/dubhe/tree/main/packages/grpc-client#readme",
  "bugs": "https://github.com/0xobelisk/dubhe/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xobelisk/dubhe.git"
  },
  "license": "Apache-2.0",
  "author": "team@obelisk.build",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:tsup",
    "build:tsup": "tsup ./src/index.ts --format esm,cjs --sourcemap",
    "build:types": "tsc --build",
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "generate": "rm -rf src/proto && mkdir src/proto && pnpm generate:proto && pnpm lint:fix",
    "generate:proto": "protoc --ts_out=./src/proto --ts_opt server_none -I ../../crates/dubhe-indexer-grpc/proto ../../crates/dubhe-indexer-grpc/proto/dubhe_grpc.proto",
    "lint": "eslint . --ext .ts",
    "prettier:check": "prettier -c --ignore-unknown .",
    "prettier:fix": "prettier -w --ignore-unknown .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@protobuf-ts/grpcweb-transport": "^2.11.1",
    "@protobuf-ts/runtime": "^2.11.1",
    "@protobuf-ts/runtime-rpc": "^2.11.1"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.11.1",
    "@types/node": "^24.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
